{
    "name": "rinvex/cortex",
    "description": "Rinvex Cortex is a solid foundation for enterprise solutions, that provides a flexible and extensible architecture for building multi-lingual, multi-tenant applications with content management, themeable views, application modules and much more.",
    "keywords": [
        "rinvex",
        "cortex",
        "flexible",
        "framework",
        "laravel",
        "themes",
        "solution",
        "extension",
        "enterprise",
        "application",
        "multi-lingual",
        "multi-tenant",
        "bootstrap",
        "module"
    ],
    "type": "project",
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/country/issues",
        "source": "https://github.com/rinvex/country",
        "docs": "https://github.com/rinvex/country/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/country/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "aws/aws-sdk-php": "~3.0",
        "aws/aws-sdk-php-laravel": "~3.0",
        "cortex/attributes": "dev-develop",
        "cortex/categories": "dev-develop",
        "cortex/console": "dev-develop",
        "cortex/contacts": "dev-develop",
        "cortex/fort": "dev-develop",
        "cortex/foundation": "dev-develop",
        "cortex/pages": "dev-develop",
        "cortex/tags": "dev-develop",
        "cortex/tenants": "dev-develop",
        "doctrine/dbal": "^2.6",
        "dusterio/laravel-aws-worker": "~0.1",
        "laravel/envoy": "~1.3.0",
        "laravel/framework": "~5.5.0",
        "laravel/tinker": "^1.0.0",
        "lord/laroute": "^2.4.0",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "predis/predis": "^1.1.0",
        "spatie/laravel-db-snapshots": "^1.1.0",
        "spatie/laravel-link-checker": "^2.0.0",
        "sven/artisan-view": "^1.3",
        "wnx/laravel-stats": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4.0",
        "barryvdh/laravel-debugbar": "^3.0.0",
        "itsgoingd/clockwork": "^1.14.0",
        "phpunit/phpunit": "^6.0.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "filp/whoops": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan laroute:generate",
            "@php artisan package:discover",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "dont-discover": [
                "spatie/laravel-menu"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
